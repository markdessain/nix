ollama: ollama serve
open-webui: WEBUI_SECRET_KEY=~/.webui_secret_key WEBUI_AUTH=false DATA_DIR=~/.openwebui open-webui serve --port 8081
tabby: tabby serve --device metal
openhands: docker run -it --rm -e SANDBOX_RUNTIME_CONTAINER_IMAGE=docker.all-hands.dev/all-hands-ai/runtime:0.37-nikolaik -e LOG_ALL_EVENTS=true -v /var/run/docker.sock:/var/run/docker.sock -v ~/.openhands:/.openhands -p 3000:3000 --add-host host.docker.internal:host-gateway  --net=host  docker.all-hands.dev/all-hands-ai/openhands:0.37
litellm: docker run -it --rm -e LITELLM_MASTER_KEY=somekey -e UI_USERNAME=someuser -e UI_PASSWORD=somepassword -v ~/.litellm/config.yaml:/app/config.yaml -v ~/.litellm/.env:/app/.env -v ~/.litellm/api-key.json:/root/.config/litellm/github_copilot/api-key.json -p 4000:4000 ghcr.io/berriai/litellm:v1.75.5-stable --config /app/config.yaml --detailed_debug